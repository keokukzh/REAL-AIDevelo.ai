###############################################
# AIDevelo.ai Environment Variables Template  #
# Copy to .env and fill with real values.     #
# Do NOT commit secrets.                      #
###############################################

# General
NODE_ENV=development
PORT=5000
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5000

# Database
DATABASE_URL=postgres://<postgres_user>:<postgres_password>@localhost:5432/aidevelo_dev
POSTGRES_USER=<postgres_user>
POSTGRES_PASSWORD=<postgres_password>
POSTGRES_DB=aidevelo_dev

# Cache / Vector DB
REDIS_URL=redis://localhost:6379
QDRANT_URL=http://localhost:6333
KNOWLEDGE_API_KEY=

# Telephony (e.g., Twilio)
TWILIO_ACCOUNT_SID=<twilio_account_sid>
TWILIO_AUTH_TOKEN=<twilio_auth_token>
TWILIO_WEBHOOK_SECRET=<twilio_webhook_secret>
TWILIO_VOICE_APP_SID=<twilio_voice_app_sid>

# ElevenLabs (server-side only)
ELEVENLABS_API_KEY=<your_elevenlabs_api_key>
ELEVENLABS_VOICE_ID_DEFAULT=<voice_id_optional>
ELEVENLABS_MODEL_ID_DEFAULT=<model_id_optional>

# Stripe / Billing (optional)
STRIPE_SECRET_KEY=<your_stripe_secret_key>
STRIPE_WEBHOOK_SECRET=<your_stripe_webhook_secret>

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# Frontend
FRONTEND_URL=http://localhost:5173
VITE_API_URL=http://localhost:5000/api
VITE_DEBUG_API=false
