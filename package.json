{
  "name": "aidevelo.ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --exclude '**/tests/e2e/**'",
    "test:integration": "cd server && npm run test:integration",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:performance": "tsx tests/framework/runPerformance.ts",
    "deploy:cf": "npm run build && wrangler pages deploy dist --project-name $CF_PAGES_PROJECT_NAME",
    "workflow:run": "tsx workflows/cli/cli.ts run",
    "workflow:validate": "tsx workflows/cli/cli.ts validate",
    "workflow:schedule": "tsx workflows/cli/cli.ts schedule",
    "workflow:monitor": "tsx workflows/cli/cli.ts monitor",
    "workflow:history": "tsx workflows/cli/cli.ts history",
    "workflow:status": "tsx workflows/cli/cli.ts status",
    "lint:changed": "node scripts/lint-changed.mjs",
    "lint:api-prefix": "node scripts/check-api-prefix.mjs"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@lottiefiles/dotlottie-web": "^0.57.0",
    "@react-three/drei": "9.96.1",
    "@react-three/fiber": "8.15.14",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "expr-eval": "^2.0.2",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "node-cron": "^4.2.1",
    "ora": "^9.0.0",
    "pg": "^8.13.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0",
    "three": "0.160.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.17",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.35",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.15",
    "wrangler": "^4.53.0"
  }
}
