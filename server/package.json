{
  "name": "aidevelo-api",
  "version": "1.0.0",
  "description": "Backend API for AIDevelo.ai",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "wait-and-migrate": "ts-node scripts/waitAndMigrate.ts",
    "migrate": "ts-node scripts/runMigrations.ts",
    "prebuild": "node scripts/prepare-shared.js",
    "build": "tsc",
    "docs:generate": "node -e \"const swagger = require('./dist/config/swagger'); const fs = require('fs'); fs.writeFileSync('./openapi.json', JSON.stringify(swagger.swaggerSpec, null, 2));\"",
    "docs:serve": "npm run build && npm start",
    "test:elevenlabs": "ts-node scripts/testElevenLabs.ts",
    "test": "vitest --config vitest.config.ts",
    "test:unit": "vitest --config vitest.config.ts run tests/unit",
    "test:integration": "vitest --config vitest.config.ts run tests/integration",
    "test:coverage": "vitest --config vitest.config.ts run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@qdrant/js-client-rest": "^1.16.2",
    "@types/multer": "^2.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "openai": "^6.10.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.0",
    "socket.io": "^4.8.1",
    "stripe": "^20.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.15",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
