# AIDevelo API - Environment Variables
# Copy this file to .env and fill in your values

# ============================================
# REQUIRED (Production)
# ============================================
NODE_ENV=
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
ELEVENLABS_API_KEY=

# ============================================
# OPTIONAL (with defaults)
# ============================================
PORT=
FRONTEND_URL=

# ============================================
# FREESWITCH (WebRTC test + call control)
# ============================================
# Public WSS URL (via Cloudflare Tunnel/Nginx), e.g. wss://freeswitch.aidevelo.ai
FREESWITCH_WSS_URL=
# Credentials used by the WebRTC test page (SIP.js). Set strong values in production.
FREESWITCH_TEST_PASSWORD=
FREESWITCH_TEST_EXTENSION=1000
# Temp dir for generated TTS wav files (must be readable by the API)
TMP_DIR=

# ============================================
# CANONICAL ENV VARS (Future Features)
# ============================================
PUBLIC_BASE_URL=
ELEVENLABS_AGENT_ID_DEFAULT=
TOOL_SHARED_SECRET=
TOKEN_ENCRYPTION_KEY=

# ============================================
# TWILIO (Phase 2)
# ============================================
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_STREAM_TOKEN=

# ============================================
# GOOGLE OAUTH (Phase 3)
# ============================================
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
GOOGLE_OAUTH_REDIRECT_URL=
